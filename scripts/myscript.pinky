-----------------------------------------------------------
-- Function to print the factorial of a number
-----------------------------------------------------------
func factorial(n)
  mul := 1
  for i := 1, n do
    mul := mul * i
  end
  ret mul
end

-----------------------------------------------------------
-- FizzBuzz
-----------------------------------------------------------
func fizzbuzz(n)
  i := 1
  while i <= n do
    if i % 3 == 0 and i % 5 == 0 then
      println "i = " + i + " -> " + "FizzBuzz"
    else
      if i %3 == 0 then
        println "i = " + i + " -> " + "Fizz"
      end
      if i % 5 == 0 then
        println "i = " + i + " -> " + "Buzz"
      end
    end
    i := i + 1
  end
end

x := factorial(6)
println x

fizzbuzz(30)
